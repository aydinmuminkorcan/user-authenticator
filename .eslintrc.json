{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["plugin:security/recommended", "airbnb-base", "prettier"],
    "ignorePatterns": ["public/**/*.js"],
    "plugins": ["prettier", "security"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
